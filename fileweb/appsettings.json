{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cms_Db_ConnectionString": "Data Source=srv-sql;Initial Catalog=Hah_Apps;Integrated Security=True;",
  "EnvOptions": {
    "PortalUrl": "http://srv-test/portal",
    "PortalUserSessionOptions": {
      "PortalUserSessionKeyName": "_Portal_Url",
      "PortalUserSessionCookieName": ".PortalUser.Session",
      "SessionExpiredMinutes": 480,
      "SessionExpireNotificationMinutes": 5,
      "LastLoginTimeSessionKeyName": "_Last_Login_Time"
    },
    "PortalUserCacheOptions": {
      "PortalUserCacheKeyName": "_Portal_User_Cache",
      "CacheExpiredHours": 8
    },
    "AppModuleOptions": {
      "ReportingModule": {
        "AlabamaModule": "ALABAMA",
        "IndianaModule": "INDIANA"
      },
      "TrainingModule": {
        "VAModule": "VA",
        "COMMERCIALModule": "COMMERCIAL"
      },
      "AdminModule": {
        "ExternalIDMappingModule": "EXTID_MAPPING"
      }
    }
  }
}
